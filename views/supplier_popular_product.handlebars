<div class="cols-xs-10 shifter">
	<h2>Supplier of Most Popular Product</h2>
	<table class="table table-striped table-bordered">
		<thead>
			<tr>
				<th>Supplier</th> <th>Product</th>
			</tr>
		</thead>
	
		{{#each data}}
			<tr>
				<td>{{shop}}</td><td>{{product}}</td>
			</tr>
		{{/each}}

	</table>
</div>