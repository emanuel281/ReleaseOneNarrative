<div class="container col-xs-12">
	<div class="cols-xs-10 shifter">
		
		<h2>Stock Rates</h2>
		<form>
			<label class="label-lg label-info">Seacrh</label>
			<input class="searchBar" id="stock_ratesSearchBar" type="text" name="stock_rates"></input>
		</form>
		<table class="table" id="stock_ratesList">
		<table class="table table-striped table-bordered">
			<thead>
				<tr>
					<th>Product</th> <th>Rate of Decrease(%)</th>
				</tr>
			</thead>
		
			{{#data}}
				<tr>
					<td>{{item}}</td><td>{{rate_as_percent}}</td>
				</tr>
			{{/data}}

		</table>
	</div>
</div>